{
  "name": "autoslice",
  "version": "0.1.0",
  "description": "Automatic audio track slicer - split full albums, vinyl rips, and live sets into individual tracks",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron .",
    "build": "electron-builder",
    "test": "node test/test-audio-engine.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.3.3",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-store": "^8.1.0",
    "electron-updater": "^6.8.3",
    "ffmpeg-static": "^5.2.0",
    "ffprobe-static": "^3.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "music-metadata": "^7.14.0",
    "node-id3": "^0.2.6",
    "wavesurfer.js": "^7.8.8"
  },
  "build": {
    "appId": "com.autoslice.app",
    "productName": "AutoSlice",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icon.png"
    },
    "asarUnpack": [
      "node_modules/ffmpeg-static/**/*",
      "node_modules/ffprobe-static/**/*"
    ]
  }
}
